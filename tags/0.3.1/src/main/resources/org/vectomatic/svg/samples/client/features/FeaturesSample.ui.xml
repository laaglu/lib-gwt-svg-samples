<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:SimplePanel ui:field='panel'>
		<g:TabPanel ui:field='tabPanel' width="100%">
			<g:Tab>
				<g:VerticalPanel>
					<g:Label text="Supported features"/>
					<g:FlexTable ui:field="table" borderWidth="1" cellSpacing="0"></g:FlexTable>		
				</g:VerticalPanel>
			</g:Tab>
			<g:Tab>
				<g:VerticalPanel>
					<g:HTML ui:field="source" />
				</g:VerticalPanel>
			</g:Tab>
		</g:TabPanel>
	</g:SimplePanel>
</ui:UiBinder>
