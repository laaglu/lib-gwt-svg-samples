<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field='mainBundle' type='org.vectomatic.svg.samples.client.Main.MainBundle' />
	<g:TabLayoutPanel ui:field='tabPanel' barUnit='EM' barHeight='2'>
		<g:tab>
			<g:customHeader><g:Label styleName="{mainBundle.css.tab}">Features</g:Label></g:customHeader>
			<g:FlowPanel>
				<g:SimplePanel>
					<g:FlowPanel styleName="{mainBundle.css.sample}">
						<g:FlexTable ui:field="table" borderWidth="1" cellSpacing="0"></g:FlexTable>		
					</g:FlowPanel>
				</g:SimplePanel>
			</g:FlowPanel>
		</g:tab>
	</g:TabLayoutPanel>
</ui:UiBinder>
