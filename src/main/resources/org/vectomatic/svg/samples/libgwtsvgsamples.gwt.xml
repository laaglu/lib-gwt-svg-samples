<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name="com.google.gwt.core.Core" />
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.dom.DOM' />
	<inherits name="org.vectomatic.libgwtsvg" />

	<!-- Inherit the default GWT style sheet.  You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                            -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

	<generate-with class="org.vectomatic.svg.samples.generator.SourceGenerator">
		<when-type-assignable class="org.vectomatic.svg.samples.client.Main.GeneratorInfo" />
	</generate-with>


	<!-- Specify the app entry point class.                         -->
	<entry-point class='org.vectomatic.svg.samples.client.Main' />
</module>
